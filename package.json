{
  "name": "Serva",
  "main": "./lib/serva",
  "version": "0.1.1",
  "description": "Run a static web server from any directory in your project.",
  "keywords": [],
  "activationCommands": {
    "atom-workspace": [
      "serva:toggle-server-list",
      "serva:stop-all-servers"
    ],
    ".tree-view": [
      "serva:start-server",
      "serva:start-watch-server",
      "serva:stop-server"
    ]
  },
  "repository": "https://github.com/jpsilva/serva",
  "license": "MIT",
  "engines": {
    "atom": ">=1.0.0 <2.0.0"
  },
  "dependencies": {
    "browser-sync": "^2.18.12",
    "gaze": "^1.1.2",
    "loophole": "^1.1.0",
    "react": "^15.6.1",
    "react-dom": "^15.6.1"
  },
  "devDependencies": {}
}
